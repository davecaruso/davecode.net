{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "postinstall": "prisma generate",
    "postbuild": "cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
    "predev": "npm run prisma:migrate:dev",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "vercel-build": "prisma generate && npm run build"
  },
  "dependencies": {
    "@davecode/structures": "^0.3.0",
    "@furudean/svelte-persistent-store": "^0.6.0",
    "@prisma/client": "^3.14.0",
    "@ungap/structured-clone": "^1.0.1",
    "blurhash": "^1.1.5",
    "blurhash-image": "^1.0.1",
    "color": "^4.2.3",
    "dotenv": "^10.0.0",
    "fluent-svelte": "^1.6.0",
    "html-minifier-terser": "^7.0.0-alpha.2",
    "mime-types": "^2.1.35",
    "prisma": "^3.14.0",
    "sharp": "^0.30.6",
    "source-map-js": "^1.0.2",
    "stacktrace-parser": "^0.1.10",
    "svelte-portal": "^2.2.0",
    "svelte-simple-markdown": "^1.0.1",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@originjs/vite-plugin-content": "^1.0.1",
    "@poppanator/sveltekit-svg": "^0.3.2",
    "@sveltejs/adapter-auto": "1.0.0-next.50",
    "@sveltejs/kit": "^1.0.0-next.348",
    "@types/color": "^3.0.3",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/sharp": "^0.30.2",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-css-order": "^1.2.0",
    "prettier-plugin-jsdoc": "^0.3.38",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.52.2",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.3"
  }
}
