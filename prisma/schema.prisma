// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Story {
  id String @id
  date DateTime
  dateEnd DateTime?
  title String
  thumb String?
  description String
  url String
  tags String[]
}