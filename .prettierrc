{
  "plugins": [
    "./node_modules/prettier-plugin-svelte",
    "./node_modules/prettier-plugin-organize-imports",
    "./node_modules/prettier-plugin-jsdoc"
  ],
  "arrowParens": "always",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "svelteBracketNewLine": false,
  "printWidth": 95,
  "tabWidth": 2,
  "trailingComma": "es5",
  "proseWrap": "always",
  "embeddedLanguageFormatting": "auto",
  "bracketSameLine": true,
  "bracketSpacing": true
}
