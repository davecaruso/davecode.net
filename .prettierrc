{
  "plugins": [
    "./node_modules/prettier-plugin-svelte",
    "./node_modules/prettier-plugin-organize-imports",
    "./node_modules/prettier-plugin-jsdoc",
    "./node_modules/prettier-plugin-css-order"
  ],
  "arrowParens": "always",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "es5",
  "proseWrap": "always",
  "embeddedLanguageFormatting": "auto",
  "bracketSameLine": true,
  "bracketSpacing": true
}
